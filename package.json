    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@types/react": "17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
